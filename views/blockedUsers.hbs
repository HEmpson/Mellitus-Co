<head>
    <link href="/css/blockuser.css" rel="stylesheet"> 
</head>

{{#each blockedUsers}}
    <div class="card" style="width: 18rem;">
        <img src="https://upload.wikimedia.org/wikipedia/commons/1/1d/Rocky_Marciano_Postcard_1953.jpg" class="unblockPhoto card-img-top " alt="image of user">
        <div class="card-body">
            <h5 class="card-title">{{this.displayName}}</h5>
            <a href="/user/unblockUser/{{this._id}}"  class="unblockButton btn btn-primary">Unblock</a>
        </div>
    </div>
{{/each}}